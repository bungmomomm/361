{
  "name": "fe-components",
  "version": "1.0.0",
  "description": "Mataharimall React Front-End Components",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/server",
    "start-watch": "nodemon --watch src --exec yarn run build-start",
    "clean": "rimraf dist",
    "dev": "better-npm-run dev",
    "analyse": "better-npm-run analyse",
    "component": "babel-node ./tools/component/index.js",
    "build-client": "better-npm-run build-client",
    "build-static": "better-npm-run build-static",
    "build-static-staging": "better-npm-run build-static-staging",
    "build-server": "better-npm-run build-server",
    "build-start": "yarn run build && yarn start",
    "build": "better-npm-run build",
    "eslint": "eslint -c .eslintrc.json \"./src/**/*.js\"",
    "lint": "yarn run eslint",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mataharimall/fe-components.git"
  },
  "author": "Vinensius Wibowo <hello@vinensiuswibowo.com> (http://vinensiuswibowo.com/)",
  "license": "MIT",
  "homepage": "https://github.com/mataharimall/fe-components#readme",
  "betterScripts": {
    "dev": {
      "command": "babel-node tools/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3001
      }
    },
    "analyse": {
      "command": "babel-node tools/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "analyse",
        "PORT": 3001
      }
    },
    "build-client": {
      "command": "yarn run clean && webpack -p --progress --hide-modules --config tools/webpack-config/client.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-static": {
      "command": "yarn run clean && webpack -p --progress --hide-modules --config tools/webpack-config/client.babel.js",
      "env": {
        "NODE_ENV": "production",
        "STATIC": true
      }
    },
    "build-static-staging": {
      "command": "yarn run clean && webpack -p --progress --hide-modules --config tools/webpack-config/client.babel.js",
      "env": {
        "NODE_ENV": "staging",
        "STATIC": true
      }
    },
    "build-server": {
      "command": "webpack --progress --hide-modules --config tools/webpack-config/server.babel.js",
      "env": {
        "NODE_ENV": "production",
        "SSR": true
      }
    },
    "build": {
      "command": "yarn run lint && yarn run build-client && yarn run build-server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "axios": "^0.16.2",
    "base-64": "^0.1.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "credit-card-type": "^5.0.3",
    "dotenv-webpack": "^1.5.4",
    "es6-promise": "^4.2.4",
    "express": "4.15.3",
    "google-maps-react": "^1.1.2",
    "html-minifier": "3.5.2",
    "jwt-decode": "^2.2.0",
    "kong-hmac": "^0.2.0",
    "loadable-components": "^0.4.0",
    "lodash-humps": "^3.1.2",
    "luhn-cc": "0.0.6",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "nuka-carousel": "^3.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^15.6.1",
    "react-cookie": "^2.0.8",
    "react-dom": "^15.6.1",
    "react-helmet": "5.1.3",
    "react-input-range": "^1.3.0",
    "react-maskedinput": "^4.0.1",
    "react-native-md5": "^1.0.0",
    "react-recaptcha": "^2.3.7",
    "react-redux": "5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-transition": "^1.2.1",
    "react-svg-inline": "^2.1.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-api-middleware": "^2.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "2.2.0",
    "regenerator": "^0.12.3",
    "remove": "^0.1.5",
    "sass-mq": "^3.3.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "swipe-react": "^1.0.4",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-export-extensions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "better-npm-run": "0.0.15",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "ejs-loader": "^0.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.7",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.6.0",
    "inquirer": "^3.2.1",
    "lodash": "^4.17.5",
    "node-sass": "^4.5.3",
    "node-sass-json-importer": "^3.1.2",
    "nodemon": "^1.14.12",
    "postcss": "^6.0.17",
    "postcss-loader": "^2.1.0",
